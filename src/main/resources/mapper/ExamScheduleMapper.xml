<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="stu.mapper.ExamScheduleMapper">

    <resultMap id="BaseResultMap" type="stu.pojo.ExamSchedule">
            <id property="courseId" column="course_id" jdbcType="VARCHAR"/>
            <result property="examTime" column="exam_time" jdbcType="TIMESTAMP"/>
            <result property="roomNumber" column="room_number" jdbcType="VARCHAR"/>
            <result property="invigilatorId" column="invigilator_id" jdbcType="INTEGER"/>
    </resultMap>

    <sql id="Base_Column_List">
        course_id,exam_time,room_number,
        invigilator_id
    </sql>
</mapper>
